{
  "name": "js-cache-tags",
  "description": "Simple and fast memory cache with tags support for Javascript/NodeJS projects.",
  "keywords": [
    "cache tags",
    "cachetags",
    "javascript cache tags",
    "js cache tags",
    "node cache tags",
    "memcached tags",
    "object cache tags",
    "memory cache tags"
  ],
  "tags": [
    "cache tags",
    "cachetags",
    "javascript cache tags",
    "js cache tags",
    "node cache tags",
    "memcached tags",
    "object cache tags",
    "memory cache tags"
  ],
  "version": "1.2.0",
  "author": "Viresh Shah <vireshshah@gmail.com>",
  "maintainers": [
    {
      "name": "Viresh Shah",
      "email": "vireshshah@gmail.com",
      "url": "https://github.com/vireshshah"
    }
  ],
  "main": "./dist/js-cache-tags.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/vireshshah/js-cache-tags.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.4.6"
  },
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "lodash.isobject": "3.0.2",
    "lodash.some": "4.6.0",
    "lodash.template": "4.4.0",
    "node-cache": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.5"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "verbose": true,
    "bail": true,
    "notify": true,
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ]
  }
}
